<module>
    <id>core-genericfile</id>
    <name>Generic File</name>
    <author>Core</author>
    <description>A generic file to be stored in the library</description>
    <class></class>
    <type>genericfile</type>
    <dataType></dataType>
    <schemaVersion>1</schemaVersion>
    <assignable>0</assignable>
    <regionSpecific>0</regionSpecific>
    <renderAs></renderAs>
    <defaultDuration>10</defaultDuration>
    <settings>
        <property id="validExtensions" type="text">
            <title>Valid Extensions</title>
            <helpText>The Extensions allowed on files uploaded using this module. Comma Separated.</helpText>
            <default>apk,ipk,js,html,htm</default>
        </property>
    </settings>
    <allowPreview>0</allowPreview>
    <properties></properties>
    <preview></preview>
    <stencil>
        <twig><![CDATA[]]></twig>
    </stencil>
</module>