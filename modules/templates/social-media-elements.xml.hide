<templates>
    <template>
        <id>profile-photo</id>
        <type>element</type>
        <dataType>social-media</dataType>
        <title>Profile Photo</title>
        <thumbnail>fulltime-np</thumbnail>
        <properties>
            <property></property>
        </properties>
        <stencil>
            <hbs><![CDATA[
<div class="twitter-profile" style="color:{{color}}"></div>
            ]]></hbs>
        </stencil>
        <assets>
            <asset id="fulltime-np" type="path" mimeType="image/png" cmsOnly="true" path="/modules/assets/template-thumbnails/social-media/fulltime-np.png" />
        </assets>
    </template>
    <template>
        <id>tweet1</id>
        <type>element-group</type>
        <dataType>social-media</dataType>
        <title>Tweet</title>
        <stencil>
            <width></width>
            <height></height>
            <elements>
                <element id="profile-photo">
                    <top>150</top>
                    <left>350</left>
                    <defaultProperties>
                        <property id=""></property>
                    </defaultProperties>
                </element>
            </elements>
        </stencil>
    </template>
</templates>